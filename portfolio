    <script>
        function show(){

            if(cert==1){
                document.getElementById("cert").style.display="block"
                document.getElementById("posium").style.display="none"
                document.getElementById("ship").style.display="none"
                document.getElementById("rank").style.display="none"

                return cert=0
            }else{
                document.getElementById("cert").style.display="none"
                return cert=1
            }
        }
        function sy(){
            if(posium==1){
                document.getElementById("posium").style.display="block"
                document.getElementById("cert").style.display="none"
                document.getElementById("ship").style.display="none"
                document.getElementById("rank").style.display="none"
                return posium=0
            }else{
                document.getElementById("posium").style.display="none"
                return posium=1
            }
        }
        function int(){
            if(ship==1){
                document.getElementById("ship").style.display="block"
                document.getElementById("posium").style.display="none"
                document.getElementById("cert").style.display="none"
                document.getElementById("rank").style.display="none"
                return ship=0
            }else{
                document.getElementById("ship").style.display="none"
                return ship=1
            }
        }
        function hac(){
            if(rank==1){
                document.getElementById("rank").style.display="block"
                document.getElementById("posium").style.display="none"
                document.getElementById("ship").style.display="none"
                document.getElementById("cert").style.display="none"
                return rank=0
            }else{
                document.getElementById("rank").style.display="none"
                return rank=1
            }
        }
        
    </script>
